{
    "type": "mysql",
    "host": "localhost",
    "port": 3306,
    "username": "root",
    "password": "",
    "database": "IntProgDB",
    "synchronize": true,
    "logging": true,
    "entities": [
        "src/entities/*.ts"
    ],
    "migrations": [
        "src/migrations/*.ts"
    ],
    "subscribers": [
        "src/subscribers/*.ts"
    ],
    "compilerOptions": {
        "target": "ES6",
        "module": "CommonJS",
        "outDir": "./dist",
        "rootDir": "./",
        "strict": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    }
}